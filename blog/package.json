{
  "name": "blog.clydedsouza.net",
  "version": "2.4.0",
  "private": true,
  "scripts": {
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build:production": "cross-env EXPORT=1 UNOPTIMIZED=1 INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "lint": "next lint --fix",
    "serve": "npm run build:production && npx serve out",
    "lint:frontmatter": "node ./scripts/lintFrontmatter.js",
    "optimize:post-images": "imagemin public/static/posts --out-dir=public/static/posts --plugin.mozjpeg.quality=40 --plugin.gifsicle.optimizationLevel=2 --plugin.pngquant.quality=0.4 --plugin.pngquant.quality=0.4",
    "optimize:site-images": "imagemin public/static/images --out-dir=public/static/images --plugin.mozjpeg.quality=40 --plugin.gifsicle.optimizationLevel=2 --plugin.pngquant.quality=0.4 --plugin.pngquant.quality=0.4",
    "optimize:site-favicons": "imagemin public/static/favicons --out-dir=public/static/favicons --plugin.mozjpeg.quality=40 --plugin.gifsicle.optimizationLevel=2 --plugin.pngquant.quality=0.4 --plugin.pngquant.quality=0.4",
    "optimize:site-ads": "imagemin public/static/ads --out-dir=public/static/ads --plugin.mozjpeg.quality=40 --plugin.gifsicle.optimizationLevel=2 --plugin.pngquant.quality=0.4 --plugin.pngquant.quality=0.4",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@next/bundle-analyzer": "15.2.4",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.5",
    "@tailwindcss/typography": "^0.5.15",
    "body-scroll-lock": "^4.0.0-beta.0",
    "contentlayer2": "0.5.5",
    "esbuild": "0.25.2",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "hast-util-from-html-isomorphic": "^2.0.0",
    "image-size": "2.0.1",
    "mixpanel-browser": "^2.54.0",
    "next": "15.2.4",
    "next-contentlayer2": "0.5.5",
    "next-share": "^0.27.0",
    "next-themes": "^0.4.6",
    "pliny": "0.4.1",
    "postcss": "^8.4.24",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-tweet": "^3.2.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.3.0",
    "rehype-katex": "^7.0.0",
    "rehype-katex-notranslate": "^1.1.4",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@svgr/webpack": "^8.0.1",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/mdx": "^2.0.12",
    "@types/mixpanel-browser": "^2.49.1",
    "@types/react": "^19.0.8",
    "@typescript-eslint/eslint-plugin": "^8.12.0",
    "@typescript-eslint/parser": "^8.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.0",
    "globals": "^15.12.0",
    "imagemin-cli": "^8.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.1.3"
  }
}